{
	"name": "@scottlz0310/renovate-config-init",
	"version": "1.0.0",
	"description": "CLI tool to initialize Renovate configuration with auto-detection",
	"type": "module",
	"bin": {
		"renovate-config-init": "./dist/index.js"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsc",
		"dev": "tsx src/index.ts",
		"tsc:check": "tsc --noEmit",
		"test": "vitest run --root ./ --dir ./test --maxWorkers 1 --no-file-parallelism",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"renovate",
		"renovate-config",
		"cli",
		"init"
	],
	"author": "scottlz0310",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/scottlz0310/renovate-config.git",
		"directory": "packages/cli"
	},
	"dependencies": {
		"@clack/prompts": "^0.9.1",
		"fast-glob": "^3.3.3"
	},
	"devDependencies": {
		"@types/node": "^22.19.2",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"vitest": "^4.0.15"
	},
	"engines": {
		"node": ">=18"
	}
}

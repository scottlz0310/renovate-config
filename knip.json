{
	"$schema": "./node_modules/knip/schema.json",
	"ignoreDependencies": ["lefthook"],
	"workspaces": {
		".": {
			"entry": [
				"scripts/check-lock-consistency.js",
				"scripts/install-lefthook.js",
				"scripts/validate-presets.js"
			],
			"project": ["scripts/**/*.js"]
		},
		"packages/cli": {
			"entry": ["src/index.ts"],
			"project": ["src/**/*.ts", "test/**/*.ts"],
			"ignore": ["dist/**"]
		}
	}
}

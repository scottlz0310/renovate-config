{
	"name": "@scottlz0310/renovate-config-init",
	"version": "2.1.0",
	"description": "CLI tool to initialize Renovate configuration with auto-detection",
	"type": "module",
	"bin": {
		"renovate-config-init": "./dist/index.js"
	},
	"files": [
		"dist",
		"presets"
	],
	"scripts": {
		"build": "tsc",
		"dev": "tsx src/index.ts",
		"tsc:check": "tsc --noEmit",
		"test": "vitest run --root ./ --dir ./test --maxWorkers 1 --no-file-parallelism",
		"validate": "node scripts/validate-presets.js",
		"lint": "biome ci .",
		"format": "biome format --write .",
		"knip": "knip --no-config-hints",
		"check:ci": "pnpm run tsc:check && pnpm run lint && pnpm run knip && pnpm run validate && pnpm run test"
	},
	"keywords": [
		"renovate",
		"renovate-config",
		"shareable-config",
		"dependencies",
		"cli",
		"init"
	],
	"author": "scottlz0310",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/scottlz0310/renovate-config.git"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@clack/prompts": "^0.11.0",
		"fast-glob": "^3.3.3"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.10",
		"@types/node": "^24.10.4",
		"ajv": "^8.17.1",
		"ajv-formats": "^3.0.1",
		"knip": "^5.75.2",
		"lefthook": "^2.0.12",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"vitest": "^4.0.16"
	},
	"packageManager": "pnpm@10.26.1",
	"engines": {
		"node": ">=24.12.0"
	}
}
